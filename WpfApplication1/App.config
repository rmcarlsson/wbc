    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
  <userSettings>
    <WpfApplication1.Properties.Settings>
      <setting name="MashEfficiency" serializeAs="String">
        <value>0.8</value>
