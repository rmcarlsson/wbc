      <filter>
        <!-- Level 3 -->
      </filter>
    </appender>
    <root>
      <!-- Level 2 -->
      <level/>
      <!-- Level 3 -->
      <appender-ref/>
      <!-- Level 3 -->
      <level value="ALL"/>
      <appender-ref ref="ConsoleAppender"/>
